<view class="full">
    <swiper indicator-dots="{{indicatorDots}}"
        autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}">
        <block wx:for="{{banners}}" wx:key="*this">
          <swiper-item>
            <image src="{{item}}" class="banner-image"/>
          </swiper-item>
        </block>
      </swiper>
  </view>